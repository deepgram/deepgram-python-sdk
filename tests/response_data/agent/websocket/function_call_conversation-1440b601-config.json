{
  "name": "function_call_conversation",
  "agent_config": {
    "think": {
      "provider": {
        "type": "open_ai",
        "model": "gpt-4o-mini"
      },
      "prompt": "You are a helpful assistant with access to functions.",
      "functions": [
        {
          "name": "get_weather",
          "description": "Get current weather for a location",
          "url": "https://httpbin.org/post",
          "method": "POST",
          "parameters": {
            "type": "object",
            "properties": {
              "location": {
                "type": "string",
                "description": "City name"
              }
            },
            "required": [
              "location"
            ]
          }
        }
      ]
    },
    "speak": {
      "provider": {
        "type": "deepgram",
        "model": "aura-2-thalia-en"
      }
    },
    "listen": {
      "provider": {
        "type": "deepgram",
        "model": "nova-3"
      }
    },
    "language": "en"
  },
  "inject_messages": [
    "What's the weather like in San Francisco?"
  ],
  "expected_events": [
    "Welcome",
    "SettingsApplied",
    "FunctionCallRequest",
    "ConversationText",
    "AgentStartedSpeaking"
  ]
}